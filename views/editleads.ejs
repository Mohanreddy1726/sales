                <option value="Advertisement"> Advertisement</option>
                <option value="Cold Call"> Cold Call</option>
                <option value="Partner"> Partner</option>
                <option value="Web Research"> Web Research</option>
              </select>
            </div>
            <div class="flex-child">
              <label for="Lead">Lead Status</label>

              <select name="leadstatus" id="Lead">
                <option value="<%= leads.leadstatus %>"><%= leads.leadstatus %></option>
                <option value="Attempted to contact"> Attempted to Contact</option>
                <option value="Contact in future"> Contact in Future</option>
                <option value="Contacted"> Contacted</option>
                <option value="Not Contacted"> Not Contacted</option>
              </select>
            </div>
          </div>
        </div>
        <div class="form-inline">
          <div>
            <label for="Lead">Rating</label>
            <select name="rating" id="Lead">
              <option value="<%= leads.rating %>"><%= leads.rating %></option>
              <option value="Acquired"> Acquired</option>
              <option value="Active"> Active</option>
